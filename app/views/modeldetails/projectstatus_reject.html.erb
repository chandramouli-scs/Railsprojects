<div class="container-fluid">
  <%= render partial: 'modeldetails/sidebar' %>
  <main class="col-12 col-md-10 col-sm-12 p-0 container-fluid'">
    <%= render 'notice' %>

    <h3 class="text-danger ml-5 mt-5 mb-3">Rejected projects (<%= @project.count %>)</h3>

    <table class="table table-striped">
	  <thead class="text-primary">
	    <tr>
	      <th scope="col">id</th>
	      <th scope="col">Project name</th>
	      <th scope="col">created by</th>
	      <th scope="col">Status</th>
	      <th scope="col">Action</th>
	    </tr>
	  </thead>
	  <tbody>
	  	<% @project.each do |project| %>
	    <tr>
	      <td ><%= project.id %></td>
	      <td><%= project.project_name.camelcase %></td>
	      <td><%= project.user.user_name.camelcase %></td>
	      <td><span class="text-danger">Rejected</span></td>
	      <td><%= link_to "edit", projectstatus_reject_edit_path(project) %></td>
	    </tr>
	    <% end %>
	  </tbody>
	</table>
    
    	
    

	</main>
	</div>